{
    "useTabs": true,
    "tabWidth": 2,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "trailingComma": "es5",
    "insertPragma": false,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "parser": "typescript",
    "htmlWhitespaceSensitivity": "css"
}